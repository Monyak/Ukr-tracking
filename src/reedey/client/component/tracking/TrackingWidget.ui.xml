<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:FlowPanel styleName="tracking-container">
		<g:HorizontalPanel styleName="tracking-controls">
			<g:cell>
				<g:HTMLPanel styleName="tracking-fields">
					<table>
						<tr>
							<td>
								<g:HTML styleName="tracking-field-label">Bar code:</g:HTML>
							</td>
							<td>
								<g:TextBox ui:field="barCodeTextBox" styleName="tracking-field-input"></g:TextBox>
							</td>
						</tr>
						<tr>
							<td>
								<g:HTML styleName="tracking-field-label">Description:</g:HTML>
							</td>
							<td>
								<g:TextBox ui:field="nameTextBox" styleName="tracking-field-input"></g:TextBox>
							</td>
						</tr>
					</table>
				</g:HTMLPanel>
			</g:cell>
			<g:cell verticalAlignment="middle">
				<g:HTML ui:field="addButton" styleName="tracking-button icon-plus"
					title="Add"></g:HTML>
			</g:cell>
			<g:cell verticalAlignment="middle">
				<g:HTML ui:field="settingsButton" styleName="tracking-button icon-cogs"
					title="Settings"></g:HTML>
			</g:cell>
		</g:HorizontalPanel>
		<g:FlowPanel styleName="tracking-list-scroll">
			<g:ScrollPanel width="100%" height="100%">
				<g:VerticalPanel ui:field="trackingList" styleName="tracking-list"></g:VerticalPanel>
			</g:ScrollPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 