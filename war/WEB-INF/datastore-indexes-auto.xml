<?xml version="1.0" encoding="utf-8"?>
<datastore-indexes
	xmlns="http://appengine.google.com/ns/datastore-indexes/1.0"
	autoGenerate="true">

	<datastore-index kind="User" ancestor="false"> 
		<property name="id" direction="asc" />
		<property name="login" direction="asc" />
	</datastore-index>
	<datastore-index kind="UserTracking" ancestor="false">
		<property name="user_id" direction="asc" />
	</datastore-index>
	<datastore-index kind="Tracking" ancestor="false">
		<property name="barcode" direction="asc" />
		<property name="tracking_date" direction="desc" />
	</datastore-index>
</datastore-indexes>